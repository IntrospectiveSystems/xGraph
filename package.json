{
  "name": "xgraph",
  "version": "1.4.8",
  "description": "An Executable Graph Database",
  "main": "./src/xgraph.js",
  "scripts": {
    "build": "cd build && npm run build",
    "test": "xgraph -v && npm run qa && npm run cde",
    "lint": "eslint src/*.js --fix",
    "pack": "webpack --watch",
    "qa": "xgraph r --cwd ./Tests/ValidationSystem/ --verbose",
    "cde": "xgraph c --cwd ./Tests/ValidationSystem/ --verbose && xgraph d --cwd ./Tests/ValidationSystem/ --verbose && xgraph e --cwd ./Tests/ValidationSystem/ --verbose",
    "qad": "node --inspect-brk=9229 ./src/xgraph.js r --cwd ./Tests/ValidationSystem/ --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IntrospectiveSystems/xGraph.git"
  },
  "author": "Introspective Systems, LLC.",
  "bugs": {
    "url": "https://github.com/IntrospectiveSystems/xGraph/issues"
  },
  "bin": {
    "xgraph": "./src/xgraph.js",
    "broker": "./src/broker.js"
  },
  "homepage": "https://github.com/IntrospectiveSystems/xGraph#readme",
  "license": "AGPL-3.0",
  "dependencies": {
    "idb": "^3.0.2",
    "jszip": "^3.1.5",
    "minimist": "1.2.0",
    "mkdirp": "^0.5.1",
    "signale": "^1.3.0",
    "tmp": "0.0.33",
    "tree-directory": "0.1.0",
    "uuid": "3.2.1",
    "volatile": "7.0.1",
    "xgraph-viewify": "4.0.2"
  },
  "devDependencies": {
    "eslint": "5.1.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "shebang-loader": "0.0.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-visualizer-plugin": "^0.1.11",
    "webpack-visualizer-plugin-with-assets": "^0.2.13"
  }
}
