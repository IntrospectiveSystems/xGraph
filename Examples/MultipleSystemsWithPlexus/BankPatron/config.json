{
    "Sources": {
        "xGraph": "mb://xgraph.modulebroker.net"
    },
    "Modules": {
        "BankPatron": {
            "Module": "xGraph.BankPatron",
            "Source": "xGraph",
            "Par": {
                "BankAccount": "$BankServer"
            }
        },
        "BankServer": {
            "Module": "xGraph.Proxy",
            "Source": "xGraph",
            "Par": {
                "Chan": "Bank",
                "Link": "$BankPatron",
                "Role": "Client",
                "AutoSave": false,
                "Encrypt": false
            }
        }
    }
}
